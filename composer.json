{
    "name": "soyhuce/zttp",
    "description": "A developer-experience focused HTTP client, optimized for most common use cases.",
    "keywords": [
        "http",
        "guzzle"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Bastien Philippe",
            "email": "bastien.philippe@soyhuce.fr"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.0",
        "tightenco/collect": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "laravel/lumen-framework": "^5.4"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Soyhuce\\Zttp\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2.x-dev"
        }
    }
}
